{
    "extends": ["tslint:recommended", "tslint-config-prettier"],
    "linterOptions": {
        "exclude": [
            "node_modules/**",
            "coverage/**",
            "build/**",
            ".nyc_output/**"
        ],
        "rules": {
            "prettier": true,
            "object-literal-sort-keys": false
        },
        "jsRules": {
            "object-literal-sort-keys": false
        }
    }
}
